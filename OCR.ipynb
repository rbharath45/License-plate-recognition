    "def detectx (frame, model):\n",
    "    frame = [frame]\n",
    "    print(f\"[INFO] Detecting. . . \")\n",
    "    results = model(frame)\n",
    "    #results.show()\n",
    "    # print( results.xyxyn[0])\n",
    "    # print(results.xyxyn[0][:, -1])\n",
    "    # print(results.xyxyn[0][:, :-1])\n",
    "\n",
    "    labels, cordinates = results.xyxyn[0][:, -1], results.xyxyn[0][:, :-1]\n",
    "\n",
    "    return labels, cordinates"
   ]
